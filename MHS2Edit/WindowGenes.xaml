        <CheckBox Name="checkBoxGene1" Content="Locked" HorizontalAlignment="Left" Margin="10,107,0,0" VerticalAlignment="Top" Grid.Column="0" Grid.Row="0" Click="checkBoxGene1_Click"/>
        <CheckBox Name="checkBoxGene2" Content="Locked" HorizontalAlignment="Left" Margin="10,107,0,0" VerticalAlignment="Top" Grid.Column="1" Grid.Row="0" RenderTransformOrigin="-0.494,1.111" Click="checkBoxGene2_Click"/>
        <CheckBox Name="checkBoxGene3" Content="Locked" HorizontalAlignment="Left" Margin="10,107,0,0" VerticalAlignment="Top" Grid.Column="2" Grid.Row="0" Click="checkBoxGene3_Click"/>
