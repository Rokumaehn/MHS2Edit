        <ComboBox Name="comboBoxGene8" ItemsSource="{Binding Gene.NAMES}" DisplayMemberPath="Name" Margin="10,80,10,0" VerticalAlignment="Top" Grid.Column="1" Grid.Row="2" SelectionChanged="ComboBoxGene8_SelectionChanged"/>
